apiVersion: apps/v1
kind: Deployment
metadata:
  name: kubes
  namespace:vin
  labels:
    app: kube
spec:
  replicas: 1
  selector:
    matchLabels:
      app: kube
  template:
    metadata:
      labels:
        app: kube
    spec:
      containers:
      - name: kube
        image: kube:1.0.0
        ports:
        - containerPort: 5000